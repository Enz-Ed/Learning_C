#include <stdio.h>

int main(){
	int valor, x, par, impar, mais, menos;
	valor=0; par=0; impar=0; mais=0; menos=0;
	do{
		scanf("%d", &x);
		if(x%2==0)
			par+=1;
		else
			impar+=1;
		if(x>0)
			mais+=1;
		if(x<0)
			menos+=1;
		valor+=1;
	}while(valor!=5);
  
	printf("%d valor(es) par(es)\n", par);
	printf("%d valor(es) impar(es)\n", impar);
	printf("%d valor(es) positivo(s)\n", mais);
	printf("%d valor(es) negativo(s)\n", menos);
	return 0;
}
