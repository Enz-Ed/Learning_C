#include <stdio.h>

int main(){
    int T;
    int N[1000];
    int i, j;
    scanf("%d", &T);
    for(i=0; i<=999; i=i){
        for(j=0; j<T; j++){
            N[i] = j;
            printf("N[%d] = %d\n", i, N[i]);
            i++;
            if(i==1000)
                break;
        }
        if(i==1000)
            break;
    }
    return 0;
}
