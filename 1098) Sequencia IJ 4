#include <stdio.h>

int main(){
    float i, j, aux=0;
    int k, l=0;
    for(i=i; i<=2; i=i){
        i=0; i+=aux; j=1; j+=aux;
        k=0;
        for(j=j; k!=3; j++){
            if (l%5==0)
                printf("I=%.0f J=%.0f\n", i, j);
            else
                printf("I=%.1f J=%.1f\n", i, j);
            k++;
        }
        aux+=0.2;
        l++;
    }
    return 0;
}
