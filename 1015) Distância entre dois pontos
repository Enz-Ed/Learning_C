#include <stdio.h>
#include <math.h>
 
int main() {
    double x1, y1;
    scanf("%lf", &x1); scanf("%lf", &y1);
    
    double x2, y2;
    scanf("%lf", &x2); scanf("%lf", &y2);
    
    double distance;
    distance = (sqrt((x2-x1)*(x2-x1) + (y2-y1)*(y2-y1)));
    printf("%.4lf\n", distance);
    
    return 0;
}
