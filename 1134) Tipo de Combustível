#include <stdio.h>

int main(){
	char num; 
  int alc=0, gas=0, dies=0;
  
	do{
		scanf(" %c", &num);
		switch (num){
			case '1':
				alc+=1;
				break;
			case '2':
				gas+=1;
				break;
			case '3':
				dies+=1;
				break;
		}
	}while(num!='4');
  
	printf("MUITO OBRIGADO\n");
	printf("Alcool: %d\n", alc);
	printf("Gasolina: %d\n", gas);
	printf("Diesel: %d\n", dies);
	return 0;
}
