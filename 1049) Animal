#include <stdio.h>
 
int main() {
    char nome1[13], nome2[13], nome3[13];
    scanf("%s", nome1); scanf("%s", nome2); scanf("%s", nome3);
    
    if (nome3[0] == 'c') {
    	printf("aguia\n");
	}
	
	 else if (nome3[0] == 'o') {
	 	if (nome2[1] == 'v')
	 		printf("pomba\n");
	 	else if (nome2[0] == 'm')
	 		printf("homem\n");
	 	else if (nome2[1] == 'n')
	 		printf("minhoca\n");
	 }
	 
	 else if (nome3[2] == 'r') {
	 	if (nome2[0] == 'm')
	 		printf("vaca\n");
	 	else if (nome2[0] == 'i')
	 		printf("lagarta\n");
	 }
	 
	 else if (nome3[2] == 'm') {
	 	if (nome2[0] == 'i')
	 		printf("pulga\n");
	 	else if (nome2[1] == 'n')
	 		printf("sanguessuga\n");
	 }
	 
    return 0;
}
